
@{
    ViewBag.Title = "ListeClients";
}

<body>
    <table>
        <tr>
            <th>Nom</th>
            <th>Age</th>
        </tr>
        @foreach (appJeff.Models.Client client in ViewData["Clients"] as List<appJeff.Models.Client>)
        {
            <tr>
                <td>@client.Nom</td>
                <td>@client.Age</td>
            </tr>
        }
    </table>
</body>

